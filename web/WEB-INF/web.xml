<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">   
    <servlet>
        <servlet-name>acao</servlet-name>
        <servlet-class>servlet.acao</servlet-class>
    </servlet>
	<servlet>
        <servlet-name>acaousuario</servlet-name>
        <servlet-class>servlet.acaousuario</servlet-class>
    </servlet>
	<servlet>
        <servlet-name>acaotiposervico</servlet-name>
        <servlet-class>servlet.acaotiposervico</servlet-class>
    </servlet>
	<servlet>
        <servlet-name>acaosugestao</servlet-name>
        <servlet-class>servlet.acaosugestao</servlet-class>
    </servlet>
	<servlet>
        <servlet-name>acaoservico</servlet-name>
        <servlet-class>servlet.acaoservico</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>filtro</servlet-name>
        <servlet-class>servlet.filtro</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>acao</servlet-name>
        <url-pattern>/acao</url-pattern>
    </servlet-mapping>
	<servlet-mapping>
        <servlet-name>acaousuario</servlet-name>
        <url-pattern>/acaousuario</url-pattern>
    </servlet-mapping>
	<servlet-mapping>
        <servlet-name>acaotiposervico</servlet-name>
        <url-pattern>/acaotiposervico</url-pattern>
    </servlet-mapping>
	<servlet-mapping>
        <servlet-name>acaosugestao</servlet-name>
        <url-pattern>/acaosugestao</url-pattern>
    </servlet-mapping>
	<servlet-mapping>
        <servlet-name>acaoservico</servlet-name>
        <url-pattern>/acaoservico</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>filtro</servlet-name>
        <url-pattern>/filtro</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
